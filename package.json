{
  "name": "envsub",
  "version": "3.0.0",
  "description": "envsub is envsubst for NodeJS",
  "main": "envsub.js",
  "bin": {
    "envsub": "bin/envsub-global.js",
    "envsubh": "bin/envsubh-global.js"
  },
  "author": "Daniel Lewis BSc(Hons)",
  "license": "ISC",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover -x \"**/*.spec.js\" ./node_modules/mocha/bin/_mocha -- test",
    "enforcer": "./node_modules/.bin/istanbul check-coverage --statement 100 --branch 100 --function 100 --line 100",
    "build": "npm-run-all test enforcer",
    "postinstall": "test -d .git && cp gitHookPrePush.sh .git/hooks/pre-push || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danday74/envsub.git"
  },
  "bugs": {
    "url": "https://github.com/danday74/envsub/issues"
  },
  "homepage": "https://github.com/danday74/envsub#readme",
  "keywords": [
    "envsub",
    "envsubst",
    "env",
    "env template",
    "environment variable template",
    "env templating",
    "environment variable templating",
    "handlebars env",
    "handlebars environment variables",
    "docker template",
    "docker templating"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "diff": "^3.2.0",
    "handlebars": "^4.0.6",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "app-root-path": "^2.0.1",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "data-driven": "^1.3.0",
    "del": "^2.2.2",
    "eslint": "^3.17.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  }
}
